<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr> <th>Song</th><th>Singer</th><th>Added By</th><th>genre</th><th>link</th>
        <th>Likes</th><th>Actions</th></tr>

        <% items.forEach(function(item){  %>
            <tr>
            <td><%= item.song %></td>
            <td><%= item.link %></td>
            <td><%= item.add %></td>
            <td><%= item.genre %></td>
            <td><%= item.artist %></td>



         <td><i class="material-icons">favorite</i></td>
                <td><a href="/delete?song=%= item.song %>"><i class="material-icons">delete</i></a></td>

       </tr>


        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
